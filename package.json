{
  "name": "dino-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n next,icons -c cyan,magenta \"next dev\" \"pnpm run watch:icon-types\"",
    "build": "next build",
    "start": "next start",
    "generate:icon-types": "python scripts/generate_icon_types.py",
    "watch:icon-types": "chokidar \"public/icons/**/*.svg\" -c \"pnpm run generate:icon-types\" --initial",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.5.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
